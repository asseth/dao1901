{
  "name": "dao1901",
  "version": "0.3.0",
  "description": "Dao1901 is a dapp to manage a non-profit organization ruled by french law 1901",
  "repository": {
    "type": "git",
    "url": "https://github.com/asseth/dao1901"
  },
  "scripts": {
    "build": "webpack",
    "compilebuild": "npm run compile && webpack --progress --display-error-details",
    "compile": "cd tests && python3.5 compile.py ../contracts/Owned.sol && python3.5 compile.py ../contracts/Dao1901Members.sol && python3.5 compile.py ../contracts/Dao1901Votes.sol",
    "dev": "webpack-dev-server --progress",
    "linkprotocol": "npm un dao1901-contracts; cd protocol; npm link; cd ..; npm link dao1901-contracts",
    "migrate": "cd protocol && truffle migrate",
    "testUI": "jest",
    "testProtocol": "cd protocol && truffle test"
  },
  "author": "Mathieu Sornay, St√©phane Roche <rstephane@protonmail.com>, Alexandre Kurth",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^8.0.18",
    "classnames": "^2.2.5",
    "fuse-box": "^2.2.2-beta.6",
    "jest": "^20.0.4",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^10.0.0",
    "postcss-modules": "^0.8.0",
    "prop-types": "^15.5.10",
    "ramda": "^0.24.1",
    "react": "^15.6.1",
    "react-css-modules": "^4.6.0",
    "react-dom": "^15.6.1",
    "react-fa": "^4.2.0",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-redux": "^5.0.5",
    "react-redux-toastr": "^6.2.7",
    "react-router-dom": "^4.1.2",
    "react-router-redux": "^5.0.0-alpha.6",
    "react-transition-group": "^1.2.0",
    "reactstrap": "^4.8.0",
    "redux": "^3.7.1",
    "redux-devtools": "^3.4.0",
    "redux-devtools-dock-monitor": "^1.1.2",
    "redux-devtools-log-monitor": "^1.3.0",
    "redux-form": "^6.8.0",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.15.6",
    "truffle-artifactor": "^2.1.5",
    "tslib": "^1.7.1",
    "typescript": "^2.4.2",
